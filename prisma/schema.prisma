generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CenterLocations {
  id   String @id() @default(uuid())
  city String
  url  String
}

model Article {
  id      String  @id() @default(uuid())
  title   String
  img     String
  content String
  active  Boolean
}

model Question {
  id       String  @id() @default(uuid())
  question String
  answer   String
  active   Boolean
}
